{
    "name": "vuebone",
    "version": "0.1.3",
    "description": "Vuebone",
    "author": "Neil Patrick Mendoza",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/npmDude/vuebone.git"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "module": "dist/vuebone.esm.js",
    "size-limit": [
        {
            "path": "dist/vuebone.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/vuebone.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^7.0.5",
        "@types/backbone": "^1.4.15",
        "@types/lodash": "^4.14.178",
        "husky": "^7.0.4",
        "size-limit": "^7.0.5",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.1",
        "typescript": "^3.9.10"
    },
    "dependencies": {
        "axios": "^0.25.0",
        "lodash": "^4.17.21",
        "lodash-es": "^4.17.21",
        "vue": "^2.6.14"
    }
}
